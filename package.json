{
  "name": "template-vue-nuxt",
  "version": "1.0.0",
  "description": "A Nuxt.js starter pack to build app with Vue.js",
  "author": "edhbr",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "precommit": "npm run lint",
    "update": "git pull && yarn run build && pm2 restart PROJECT_NAME"
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.5.4",
    "@nuxtjs/sitemap": "^1.2.0",
    "cookie-universal-nuxt": "^2.0.17",
    "cross-env": "^5.2.0",
    "dotenv": "^8.0.0",
    "is-mobile": "^2.1.0",
    "nuxt": "^2.8.1",
    "object-fit-images": "^3.2.4"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.2",
    "eslint": "^6.1.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-loader": "^2.2.1",
    "eslint-plugin-prettier": "3.1.0",
    "eslint-plugin-vue": "^5.2.3",
    "nodemon": "^1.19.1",
    "nuxt-stylus-resources-loader": "^2.0.0",
    "prettier": "1.18.2",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2"
  }
}
