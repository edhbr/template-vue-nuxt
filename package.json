{
  "name": "template-vue-nuxt",
  "version": "2.5.0",
  "description": "A Nuxt.js starter pack to build app with Vue.js",
  "author": "lihbr",
  "license": "SEE LICENSE IN LICENSE",
  "scripts": {
    "dev": "nuxt",
    "predev:netlify": "shx rm -rf ./.nuxt",
    "dev:netlify": "netlify dev --command=\"yarn dev\"",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore --ignore-pattern src/modules/*/*.plugin.js --ignore-pattern src/app/router.scrollBehavior.js ."
  },
  "engines": {
    "node": ">=10.16.3",
    "yarn": ">=1.13.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint",
      "commit-msg": "commitlint --env HUSKY_GIT_PARAMS"
    }
  },
  "dependencies": {
    "color": "3.1.2",
    "cross-env": "7.0.2",
    "dotenv": "8.2.0",
    "is-mobile": "2.2.1",
    "isomorphic-unfetch": "3.0.0",
    "lodash": "4.17.15",
    "md5": "2.2.1",
    "nuxt": "2.12.0",
    "object-fit-images": "3.2.4",
    "prismjs": "1.19.0",
    "qs": "6.9.1",
    "scrollmagic": "2.0.7",
    "stats.js": "0.17.0",
    "tailwindcss": "1.2.0",
    "vue-functional-data-merge": "3.1.0"
  },
  "devDependencies": {
    "@commitlint/cli": "8.3.5",
    "@commitlint/config-conventional": "8.3.4",
    "@fullhuman/postcss-purgecss": "2.1.0",
    "@nuxt/common": "2.3.4",
    "@nuxtjs/eslint-module": "1.1.0",
    "@nuxtjs/feed": "1.1.0",
    "@nuxtjs/global-components": "0.6.1",
    "@nuxtjs/google-tag-manager": "2.3.2",
    "@nuxtjs/netlify-files": "1.0.0",
    "@nuxtjs/sitemap": "2.0.1",
    "@nuxtjs/style-resources": "1.0.0",
    "babel-eslint": "10.1.0",
    "eslint": "6.8.0",
    "eslint-config-prettier": "6.10.0",
    "eslint-loader": "3.0.3",
    "eslint-plugin-prettier": "3.1.2",
    "eslint-plugin-vue": "6.2.2",
    "fibers": "4.0.2",
    "globby": "11.0.0",
    "husky": "4.2.3",
    "nodemon": "2.0.2",
    "nuxt-svg-loader": "1.2.0",
    "prettier": "1.19.1",
    "sass": "1.26.3",
    "sass-loader": "7.1.0",
    "shx": "0.3.2",
    "slugify": "1.4.0"
  }
}
